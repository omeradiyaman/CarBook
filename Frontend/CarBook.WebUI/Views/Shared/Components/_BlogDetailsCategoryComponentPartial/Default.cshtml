@model List<ResultCategoryDto>
@{
    Random random = new Random();
}
<div class="sidebar-box ftco-animate">
    <div class="categories">
        <h3>Kategoriler</h3>
        @foreach (var item in Model)
        {
            <li><a href="#">@item.Name<span>(@random.Next(1, 10))</span></a></li>
        }
    </div>
</div>